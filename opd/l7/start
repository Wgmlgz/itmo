mv 6666ORG  0x1D0

RESULT: WORD 0x0

CHECK1: WORD 0x0
CHECK2: WORD 0x0
CHECK3: WORD 0x0

RES1: WORD 0x0
RES2: WORD 0xFFFF
RES3: WORD 0x7D9A

ARG1: WORD 0x0
ARG2: WORD 0xAAAA
ARG3: WORD 0xAAFF

ORG 0x01E3
START:  CALL TEST1
        CALL TEST2
        CALL TEST3
        LD #0x1
        AND CHECK1
        AND CHECK2
        AND CHECK3
        ST RESULT
STOP:   HLT 

TEST1:  LD ARG1
        WORD 0x0F01 ; XORSP
        CMP RES1
        BEQ DONE1
ERROR1: CLA
        RET
DONE1:  LD #0x1
        ST CHECK1
        CLA 
        RET 
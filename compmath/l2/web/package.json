    "app-dev": "npm run tauri dev",
    "build": "export NODE_OPTIONS=--max_old_space_size=8192; vite build && npm run dev:package",
    "preview": "vite preview --host",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "package": "npm run tauri build",
    "ppackage": "electron-builder --config electron-builder.config.json",
    "dev:package": "electron-builder --config electron-builder.config.json --dir",
    "electron": "concurrently --kill-others \"vite dev\" \"electron src/electron.cjs\"",
    "dev:electron": "electron src/electron.cjs",
    "tauri": "tauri",
    "license-check": "license-checker --csv --out web-licenses.csv"
